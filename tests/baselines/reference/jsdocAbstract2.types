/** @abstract */
class A {
>A : A

    /**
     * @abstract
     * @returns {number}
     */
    foo() {}
>foo : () => number
}

// ok
class B extends A {
>B : B
>A : A

    foo() {
>foo : () => number

        return 1;
>1 : 1
    }
}

// error
class C extends A {}
>C : C
>A : A

// ok
/** @abstract */
class D extends A {}
>D : D
>A : A

